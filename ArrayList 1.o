import java.util.ArrayList;
import java.util.Arrays;
import java.util.List;
import java.util.Collections;
class Main{
    
    
    public static void swap (ArrayList<Integer> list, int idx1, int idx2){
        int temp=list.get(idx1);
        list.set(idx1, list.get(idx2));
        list.set(idx2, temp);
    }
    public static void main(String ar[]){
        ArrayList<Integer> list=new ArrayList<>();
        list.add(1);
        list.add(9);
        list.add(2);
        list.addAll(Arrays.asList(12,13,11,17,6,15,19,13,21,3)); //TC: O(1)
        
        ArrayList<String> list2=new ArrayList<>();
        list2.add("Kavita");
        list2.add("Chandani");
        list2.add("Nandini");
        list2.addAll(Arrays.asList("Tapasya","Amit","Tanya","Akaay","Kajal","Sakshi"));//Tc:O(1)
        
        for(int num:list){
            System.out.print(num+" ");
        }
        System.out.println();
        for(int i=0; i<list2.size(); i++){
            System.out.print(list2.get(i)+" ");
        }
        
        System.out.println();
        System.out.println(list);
        System.out.println(list.get(3)); //TC:O(1)
        list.remove(4); //TCO(n)
        System.out.println(list);
        
        list.removeAll(Arrays.asList(11, 6, 3));
        System.out.println(list);
        
        System.out.println(list2.get(3));
        System.out.println(list2);
        list2.removeAll(Arrays.asList("Akaay","Kavita"));
        System.out.println(list2);
        
        
        list2.set(1, "Manav"); //TC: O(n)
        System.out.println(list2);
        
        
        List<Integer> indices = Arrays.asList(0, 3, 4, 7);
        List<Integer> newValues = Arrays.asList(77, 29, 74, 89);
        for (int i = 0; i < indices.size(); i++) {
               list.set(indices.get(i), newValues.get(i));
        }
        System.out.println(list);
        
        
        System.out.println(list2.contains("Chandani"));//TC:O(n)
        
        list.add(4, 9);//To add any element in a list at any index suppose 4.
        System.out.println(list);
        
        
        
        for(int i=list2.size()-1; i>=0; i--){
            System.out.print(list2.get(i)+" ");
        }
        System.out.println(); //Program to print elements of an arraylist in reverse 
        //TC:O(n)
        
        
        //TC: O(n)
        //Program to find maximum value element from an arraylist
        int max=Integer.MIN_VALUE;
        for(int i=0; i<list.size(); i++){
            // if(max<list.get(i)){
            //     max=list.get(i);
            // }
            max=Math.max(max,list.get(i));
        }
        System.out.println(max);
        
        
        
        System.out.println(list);
        swap(list, 3, 7);//Swap elements of an arraylist
        System.out.println(list);
        
        
        
        Collections.sort(list);//Sorting in arraylist(Ascensing order)
        System.out.println(list);
        
        Collections.sort(list, Collections.reverseOrder());//Sorting in arraylist(Descending order)
        System.out.println(list);
    }
}
